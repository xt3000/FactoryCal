<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="24dp" android:height="24dp">
        <shape android:shape="rectangle">
            <size android:width="24dp" android:height="24dp"/>
<!--            <stroke android:width="1px"/>-->
        </shape>
    </item>

    <item android:width="19dp" android:height="4dp" android:left="3dp" android:top="4dp" >
        <shape android:shape="line">
            <stroke android:color="@color/white" android:width="3dp" android:dashGap="2dp" android:dashWidth="3.5dp"/>
        </shape>
    </item>

    <item android:width="19dp" android:height="4dp" android:left="3dp" android:top="10dp">
        <shape android:shape="line">
            <stroke android:color="@color/white" android:width="3dp" android:dashGap="2dp" android:dashWidth="3.5dp"/>
        </shape>
    </item>

    <item android:width="13dp" android:height="4dp" android:left="3dp" android:top="16dp">
        <shape android:shape="line">
            <stroke android:color="@color/white" android:width="3dp" android:dashGap="2dp" android:dashWidth="3.5dp"/>
        </shape>
    </item>

    <item android:width="8dp" android:height="1.5dp" android:left="15dp" android:top="18dp">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <corners android:radius="0.5dp"/>
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

    <item android:width="8dp" android:height="1.5dp" android:left="15dp" android:top="18dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <corners android:radius="0.5dp"/>
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

</layer-list>