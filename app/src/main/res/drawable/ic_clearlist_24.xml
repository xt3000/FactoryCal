<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="24dp" android:height="24dp">
        <shape android:shape="rectangle">
            <size android:width="24dp" android:height="24dp"/>
<!--            <stroke android:width="1px"/>-->
        </shape>
    </item>

    <item android:width="18dp" android:height="2dp" android:left="1dp" android:top="4dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item android:width="18dp" android:height="2dp" android:left="1dp" android:bottom="2dp" android:gravity="center_vertical">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item android:width="10dp" android:height="2dp" android:left="1dp" android:bottom="6dp" android:gravity="bottom">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item android:width="12dp" android:height="1dp" android:right="0dp" android:bottom="4dp" android:gravity="bottom|right">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

    <item android:width="12dp" android:height="1dp" android:right="0dp" android:bottom="4dp" android:gravity="bottom|right">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

</layer-list>