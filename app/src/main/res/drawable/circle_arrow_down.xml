<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="30dp"
        android:height="30dp">
        <shape
            android:shape="oval">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

    <item
        android:height="10dp"
        android:width="10dp"
        android:right="-6dp"
        android:gravity="center">
        <rotate
            android:fromDegrees="-45">
            <shape
                android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="@color/rbActive"/>
            </shape>
        </rotate>
    </item>

    <item
        android:gravity="center"
        android:right="6dp"
        android:width="10dp"
        android:height="10dp">
        <rotate
            android:fromDegrees="45">
            <shape
                android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="@color/rbActive"/>
            </shape>
        </rotate>
    </item>

</layer-list>
