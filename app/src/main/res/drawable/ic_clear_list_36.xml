<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="36dp" android:height="36dp">
        <shape android:shape="rectangle">
            <size android:width="24dp" android:height="24dp"/>
<!--                        <stroke android:width="1px"/>-->
        </shape>
    </item>

    <item android:width="27dp" android:height="2dp" android:left="1dp" android:top="4dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimaryDark"/>
        </shape>
    </item>

    <item android:width="27dp" android:height="2dp" android:left="1dp" android:top="11dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimaryDark"/>
        </shape>
    </item>

    <item android:width="18dp" android:height="2dp" android:left="1dp" android:top="18dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimaryDark"/>
        </shape>
    </item>

    <item android:width="18dp" android:height="2dp" android:left="1dp" android:top="25dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimaryDark"/>
        </shape>
    </item>

    <item android:width="18dp" android:height="1dp" android:right="-2dp" android:bottom="8dp" android:gravity="bottom|right">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="@color/colorPrimaryDark"/>
            </shape>
        </rotate>
    </item>

    <item android:width="18dp" android:height="1dp" android:right="-2dp" android:bottom="8dp" android:gravity="bottom|right">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/colorPrimaryDark"/>
            </shape>
        </rotate>
    </item>

</layer-list>