<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="30dp"
        android:height="2dp">
        <shape
            android:shape="rectangle">
        </shape>
    </item>

    <item
        android:width="2dp"
        android:height="2dp"
        android:gravity="center"
        android:right="40dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/rbActive"/>
        </shape>
    </item>

    <item
        android:height="2dp"
        android:width="2dp"
        android:right="20dp"
        android:gravity="center">
        <shape
            android:shape="oval">
            <solid android:color="@color/rbActive"/>
        </shape>
    </item>

    <item
        android:width="2dp"
        android:height="2dp"
        android:gravity="center">
        <shape
            android:shape="oval">
            <solid android:color="@color/rbActive"/>
        </shape>
    </item>



    <item
        android:width="2dp"
        android:height="2dp"
        android:gravity="center"
        android:left="20dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/rbActive"/>
        </shape>
    </item>

    <item
        android:width="2dp"
        android:height="2dp"
        android:gravity="center"
        android:left="40dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/rbActive"/>
        </shape>
    </item>

</layer-list>
